; DIPALMITOYL PHOSPHATIDYLCHOLINE
; in general models PCs with saturated tail lengths C15-18

[ moleculetype ]
; molname  nrexcl
  DPPC     1

[ atoms ]
; i type resnr residue atom cgnr charge
  1 Q0   1     DPPC    NC3   1    1.0
  2 Qa   1     DPPC    PO4   2   -1.0
  3 Na   1     DPPC    GL1   3    0.0
