 10 C1   1     DPPC    C2B  10    0.0
 11 C1   1     DPPC    C3B  11    0.0
 12 C1   1     DPPC    C4B  12    0.0

[ bonds ]
; i  j  funct  alias
  1  2  1      mb_np
  2  3  1      mb_pg1
  3  4  1      mb_gg
  3  5  1      mb_cc
  5  6  1      mb_cc
  6  7  1      mb_cc
  7  8  1      mb_cc
